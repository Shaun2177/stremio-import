# Example environment file
# Copy this to .env and fill in your actual values

# Your Stremio account credentials
EMAIL=your_email@example.com
PASSWORD=your_stremio_password_here

# Scheduling options
SCHEDULE_SECONDS=7200     # Run every 7200 seconds (2 hours). Examples: 3600=1hr, 7200=2hr

# Browser configuration (optional - defaults to '/usr/bin/chromium-browser')
PUPPETEER_EXECUTABLE_PATH=/usr/bin/chromium-browser